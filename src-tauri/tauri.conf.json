{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ox-mythoras",
  "version": "0.1.0",
  "identifier": "com.xenonheat.ox-mythoras",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "ox-mythoras",
        "width": 1280,
        "height": 720,
        "minWidth": 1280,
        "minHeight": 720
      }
    ,
      {
        "label": "devtools",
        "title": "ox-mythoras-devtools",
        "width": 480,
        "height": 720,
        "resizable": true,
        "visible": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
